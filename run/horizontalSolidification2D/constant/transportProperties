FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    transportModel  Newtonian;
    nu              5.65e-07;
    rho             2475;

    quasiIncompressible true;

    rhoSolid        2700;
    betaLiquid 	    -1.414e-04;;			
    tauCorr         1e-4; //Match from controlDict
}

gas
{
    transportModel  Newtonian;

    nu              1.e-4;
    rho             0.4;
}

sigma
    {
        type            constant;
        sigma           0.87;
    }

  surfaceTensionForceModel RDF;
  accelerationForceModel gravity;
  deltaFunctionModel alphaCSF;

beta            beta            [ K^-1 ]     0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ] 5e+7;
qMushyCoeff     qMushyCoeff                  1e-15;

