FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.metal;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    left
    {
        type            fixedValue;
	    value		    uniform 1;
    }

    right
    {
        type            fixedValue;
        value           uniform 1;
    }

    defaultFaces
    {
         type            empty;
    }
}

