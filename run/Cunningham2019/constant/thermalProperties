FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Sources:
// K. C. Mills, Recommended Values of Thermophysical Properties for Selected Commercial Alloys, 2002
//

metal
{
    molWeight       47.86; // for Ti

    Tmelting        1923; // Mills Ti64
    Tboiling        3558; // Brangers, Brook pure Ti
    Tcritical       4500; // maximum temperature in Clausius-Clapeyron equation

    Hfusion         2.86e5; // Mills Ti64
    Hvapour         9.82e6; // Check...now from Zenz


    solid
    {
        kappaCoeffs (2.1 1.44e-2);   // Mills Ti64
        CpCoeffs    (560 9.3e-2);     // Mills Ti64 with phase transform
    }

    liquid
    {
        kappaCoeffs (-6.6 1.83e-2);     // Boivineau
        CpCoeffs    (830 0);            // Mills Ti64
    }
}

gas
{
    molWeight       39.95; // for Argon
    kappaCoeffs     (2.1527e-2 2.67e-5);
    CpCoeffs        (830 0);
}

sigmoid         erf; // tanh or cut

solidificationFields
{
    enabled     no; // compute and write solidification(Gradient|Speed)
    alphaTol    0.5;
}

// ************************************************************************* //
