FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    transportModel  Newtonian; //trueArrheniusNewtonian
    nu              11.4e-07;   // Wunderlich: nu = 11.4e-07, mu = 4.76e-3
    rho             4170;       // Mills Ti64 at 1650

    quasiIncompressible true;

    rhoSolid        4170;
    dRhoDT.solid    (0 0);
    dRhoDT.liquid   (0  0);
    tauCorr             1e10;
}

gas
{
    transportModel  Newtonian;

    nu              2.76e-4;
    rho             2.9;
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial
    (
        (1.33                         0)
        (-1e-04                       1)
   );
}


surfaceForces
{
  sigma
 {
   	 type            temperatureDependent;
   	 sigma           polynomial
   	 (
        (1.33                         0)
        (-1e-04                       1)
  	 );
  }
  surfaceTensionForceModel fitParaboloid;
  accelerationForceModel gravity;
  deltaFunctionModel alphaCSF;
}

beta            beta            [ K^-1 ]          0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ]     5e+8;
qMushyCoeff     qMushyCoeff                     1e-15;

