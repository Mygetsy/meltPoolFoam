FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    //transportModel  Newtonian;
    transportModel  trueArrheniusNewtonian;
    trueArrhenius<Newtonian>Coeffs
    {
        alpha       5490;
        Talpha      1700;
    };
    nu              8.56e-07;   // Kim: nu = 8.56e-07, mu = 6.42e-3
    rho             7256;       // Kim: at T = 1700

    quasiIncompressible true;

    rhoSolid        7256;
    betaLiquid      0;
    tauCorr             1e12;
}

gas
{
    transportModel  Newtonian;

    nu              2.76e-4;
    rho             2.9;
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial
    (
        (1.5                            0)
        (12.5e-05                           1)
        (0.0                                2)
   );
}


surfaceTensionForceModel fitParaboloid;
accelerationForceModel gravity;
deltaFunctionModel alphaCSF;


beta            beta            [ K^-1 ]          0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ]     5e+3;
qMushyCoeff     qMushyCoeff                     1e-15;

